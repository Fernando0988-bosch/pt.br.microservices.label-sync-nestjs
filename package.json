{
  "name": "@pt.br.microservices.label-sync-nestjs/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "start": "nx serve",
    "build": "nx build",
    "build:all": "nx run-many --target=build --all",
    "serve:sap-catalog-poller": "tsx apps/sap-catalog-poller/src/main.ts",
    "build:sap-catalog-poller": "nx build sap-catalog-poller",
    "dev:sap-catalog-poller": "nodemon",
    "prod:sap-catalog-poller": "nx build sap-catalog-poller --configuration=production && node dist/apps/sap-catalog-poller/main.js",
    "serve:edge-broker": "tsx apps/edge-broker/src/main.ts",
    "build:edge-broker": "nx build edge-broker",
    "dev:edge-broker": "nodemon --exec 'tsx apps/edge-broker/src/main.ts' --watch 'apps/edge-broker/src' --watch 'libs' --ext ts",
    "prod:edge-broker": "nx build edge-broker --configuration=production && node dist/apps/edge-broker/main.js",
    "serve:label-config-api": "tsx apps/label-config-api/src/main.ts",
    "build:label-config-api": "nx build label-config-api",
    "dev:label-config-api": "nodemon --exec 'tsx apps/label-config-api/src/main.ts' --watch 'apps/label-config-api/src' --watch 'libs' --ext ts",
    "prod:label-config-api": "nx build label-config-api --configuration=production && node dist/apps/label-config-api/main.js",
    "serve:print-orchestrator": "tsx apps/print-orchestrator/src/main.ts",
    "build:print-orchestrator": "nx build print-orchestrator",
    "dev:print-orchestrator": "nodemon --exec 'tsx apps/print-orchestrator/src/main.ts' --watch 'apps/print-orchestrator/src' --watch 'libs' --ext ts",
    "prod:print-orchestrator": "nx build print-orchestrator --configuration=production && node dist/apps/print-orchestrator/main.js",
    "test": "nx test",
    "test:all": "nx run-many --target=test --all",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "lint:all": "nx run-many --target=lint --all",
    "lint:affected": "nx affected:lint",
    "lint:check": "eslint . --max-warnings 0",
    "prettier": "prettier --check . --ignore-unknown",
    "prettier:fix": "prettier --write . --ignore-unknown",
    "prettier:all": "nx format:write",
    "prettier:affected": "nx affected:format",
    "prettier:check": "nx format:check"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.3",
    "@nx/eslint": "^21.2.1",
    "@nx/eslint-plugin": "^21.2.1",
    "@nx/jest": "21.2.1",
    "@nx/js": "^21.2.1",
    "@nx/linter": "^19.8.4",
    "@nx/nest": "^21.2.1",
    "@nx/node": "^21.2.1",
    "@nx/web": "21.2.1",
    "@nx/webpack": "21.2.1",
    "@nx/workspace": "21.2.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/amqplib": "^0.10.7",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "nx": "21.2.1",
    "prettier": "^3.6.0",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.3.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.8",
    "axios": "^1.6.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
